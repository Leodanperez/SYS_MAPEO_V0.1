CIP = NPERSONAL_NROADMIN
AND PER_PERFILES.CPERFILES_CODPERFIL = PER_USUARIOS_PER_PERFILES.CPERFILES_CODPERFIL
AND PER_PERFILES.CPERFILES_CODPERFIL = PER_PERFILES_PER_MODULOS.CPERFILES_CODPERFIL
