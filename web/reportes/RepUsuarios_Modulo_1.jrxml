WHEN 'I' THEN 'INACTIVO'
END
from V_USUARIOS_SISTEMA WHERE CIP = NPERSONAL_NROADMIN) AS ESTAD,

(select REPLACE(VPERSONAL_APELLNOM,'/',' ') from PER_LRMILITARES where CMILITARES_CIP='331843500') as kkk 

